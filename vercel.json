{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "prisma generate && next build",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "regions": ["jnb1"],
  "crons": [
    {
      "path": "/api/cron/daily-reports",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/aol-check",
      "schedule": "0 9 * * *"
    }
  ]
}
